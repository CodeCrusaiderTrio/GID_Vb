function check_partitions(e,d){var c=document.getElementById("partition-"+e+"-1");var b=document.getElementById("partition-"+e+"-2");var a=document.getElementById("partition-"+e+"-3");var i=(1*document.getElementById("partition-"+e+"-1").value);var h=(1*document.getElementById("partition-"+e+"-2").value);var g=(1*document.getElementById("partition-"+e+"-3").value);check_numeric(c,i,"1",d);check_numeric(b,h,"2",d);check_numeric(a,g,"3",d);var i=(1*document.getElementById("partition-"+e+"-1").value);var h=(1*document.getElementById("partition-"+e+"-2").value);var g=(1*document.getElementById("partition-"+e+"-3").value);var f=(i+h+g);if(f>d){document.getElementById("total_"+e).innerHTML=f+"GB Total (Exceeds "+d+"GB)";document.getElementById("total_"+e).style.color="#FF0000";document.getElementById("partition_pass").value=0}else{if(f<d){document.getElementById("total_"+e).innerHTML=f+"GB / "+d+"GB";document.getElementById("total_"+e).style.color="#FF0000";document.getElementById("partition_pass").value=0}else{document.getElementById("total_"+e).innerHTML=f+"GB Total";document.getElementById("total_"+e).style.color="#2eb58c";document.getElementById("partition_pass").value=1}}}function check_numeric(b,d,c,a){b.style.color="#000000";if(isNaN(parseFloat(d))&&!isFinite(d)){pcsAlert(182);b.value="0";b.style.color="#FF0000";b.focus();return false}else{if(d%1!=0){pcsAlert(183);b.value="0";b.style.color="#FF0000";b.focus();return false}else{if(d==""){b.value="0";return false}else{if(d<80&&c==1){pcsAlert(184);b.value="80";b.style.color="#FF0000";b.focus();return false}else{if(d<10&&d!="0"&&a%10==0){pcsAlert(185);b.value="10";b.style.color="#FF0000";b.focus();return false}else{if(d%10!=0&&a%10==0){pcsAlert(186);b.value="0";b.style.color="#FF0000";b.focus();return false}}}}}}};